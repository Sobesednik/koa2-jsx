{
  "name": "koa2-jsx",
  "version": "1.0.0",
  "description": "A Koa2 middleware to render JSX templates with Redux support",
  "main": "build",
  "scripts": {
    "test": "zoroaster build/test/spec",
    "test:watch": "zoroaster build/test/spec --watch",
    "lint": "eslint .",
    "build": "babel src --out-dir build/src",
    "build:test": "babel test --out-dir build/test",
    "example": "node example/require"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/artdecocode/koa2-jsx.git"
  },
  "keywords": [
    "koa2",
    "jsx",
    "view",
    "template",
    "react",
    "redux",
    "server",
    "backend",
    "universal"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artdecocode/koa2-jsx/issues"
  },
  "homepage": "https://github.com/artdecocode/koa2-jsx#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.40",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0-beta.40",
    "@babel/preset-react": "^7.0.0-beta.40",
    "@babel/register": "^7.0.0-beta.40",
    "babel-plugin-react-require": "^3.0.0",
    "eslint": "^4.18.2",
    "eslint-plugin-react": "^7.7.0",
    "koa": "^2.5.0",
    "koa-static": "^4.0.2",
    "rqt": "^1.0.1",
    "source-map-support": "^0.5.3",
    "zoroaster": "^1.0.0-beta.2"
  },
  "dependencies": {
    "html": "1.0.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.7",
    "redux": "3.7.2"
  }
}
